    "autoprefixer": "^7.1.2",
    "browser-sync": "^2.18.13",
    "cpx": "^1.5.0",
    "gh-pages": "^1.0.0",
    "node-sass": "^4.5.3",
    "postcss-cli": "^4.1.0"
  },
  "dependencies": {}
}
